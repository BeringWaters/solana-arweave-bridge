{
  "name": "solana_arweave_bridge",
  "version": "1.0.0",
  "description": "This project is a bridge to connect Solana ledger data to Arweave permanent storage",
  "main": "index.js",
  "repository": "git@github.com:exyte/solana_arweave_bridge.git",
  "author": "Alexey Khmelev <alexey@ubikgroup.co>",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node -e 'require(\"./dist/index.js\").start()'",
